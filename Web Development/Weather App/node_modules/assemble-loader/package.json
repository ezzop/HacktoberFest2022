{
  "name": "assemble-loader",
  "description": "Assemble plugin (^0.6.0) for loading globs of views onto custom view collections. Also works with verb or other Templates.js based applications.",
  "version": "0.2.6",
  "homepage": "https://github.com/jonschlinkert/assemble-loader",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/assemble-loader",
  "bugs": {
    "url": "https://github.com/jonschlinkert/assemble-loader/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "file-contents": "^0.2.4",
    "has-glob": "^0.1.1",
    "is-valid-glob": "^0.3.0",
    "isobject": "^2.0.0",
    "lazy-cache": "^1.0.2",
    "load-templates": "^0.10.1",
    "mixin-deep": "^1.1.3"
  },
  "devDependencies": {
    "engine-base": "^0.1.2",
    "engine-handlebars": "^0.6.1",
    "gulp": "^3.9.0",
    "gulp-eslint": "^1.1.1",
    "gulp-istanbul": "^0.10.0",
    "gulp-mocha": "^2.1.3",
    "mocha": "*",
    "parser-front-matter": "^1.2.5",
    "should": "*",
    "templates": "^0.8.1"
  },
  "keywords": [
    "assemble",
    "assembleplugin",
    "blog",
    "collection",
    "page",
    "plugin",
    "post",
    "site",
    "template",
    "templates",
    "view"
  ],
  "verb": {
    "related": {
      "list": [
        "assemble",
        "templates",
        "verb"
      ]
    },
    "plugins": [
      "gulp-format-md"
    ]
  }
}
